{
    "name": "orchestra/publisher",
    "description": "Publisher Component for Orchestra Platform",
    "keywords": ["laravel", "orchestral", "orchestral-platform", "migration", "asset-publishing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Publisher\\" : "src/"
        }
    },
    "require": {
        "php": ">=5.6.0",
        "orchestra/contracts": "~3.4.0",
        "illuminate/support": "~5.3.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "illuminate/console": "~5.3.0",
        "illuminate/container": "~5.3.0",
        "illuminate/database": "~5.3.0",
        "illuminate/filesystem": "~5.3.0"
    },
    "suggest": {
        "laravel/framework": "Allow using orchestra/extension component outside of Orchestra Platform (~5.4).",
        "orchestra/foundation": "Allow using orchestra/extension with Orchestra Platform (~3.4)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.4-dev"
        }
    },
    "minimum-stability": "dev"
}
