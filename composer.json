{
    "name": "orchestra/publisher",
    "description": "Publisher Component for Orchestra Platform",
    "keywords": ["laravel", "orchestral", "orchestral-platform", "migration", "asset-publishing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Publisher\\" : "src/"
        }
    },
    "require": {
        "php": ">=7.0",
        "illuminate/support": "~5.6.0",
        "orchestra/contracts": "~3.6.0"
    },
    "require-dev": {
        "illuminate/console": "~5.6.0",
        "illuminate/container": "~5.6.0",
        "illuminate/database": "~5.6.0",
        "illuminate/filesystem": "~5.6.0",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "~6.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.6-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
